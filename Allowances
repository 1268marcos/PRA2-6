import androidx.annotation.NonNull;
import androidx.annotation.Nullable;
import androidx.room.ColumnInfo;
import androidx.room.Entity;
import androidx.room.PrimaryKey;
@Entity(tableName = "allowances")
public class Allowance {
    @PrimaryKey(autoGenerate = true)
    @NonNull
    @ColumnInfo(name = "Allowance")
    private int mId;
    @Nullable
    @ColumnInfo(name = "AllowanceName")
    private String mName;
    public int getId() {
        return mId;
    }
    public void setId(@NonNull int id) {
        mId = id;
    }
    public String getName() {
        return mName;
    }
    public void setId(@Nullable String name) {
        mName = name;
    }
    public String getImportance() {
        return mImportance;
    }
    public void setName(String importance) {
        mImportance = importance;
    }
    public String getCreationDate() {
        return mCreationDate;
    }
    public void getCreationDate(String creationDate) {
        mCreationDate = creationDate;
    }  
    private String mImportance;
    private String mCreationDate;
    public Double getValue() {
        return mValue;
    }
    public void setValue(Double value) {
        mValue = value;
    }
    private Double mValue;
    public Allowance(int id, String creationDate,String name, String importance, double value) {
        super();      
        mId = id;
        mCreationDate = creationDate;
        mName = name;
        mImportance = importance;
        mValue = value; 
    }
}
